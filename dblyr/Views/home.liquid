<h1>dblyr information</h1>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">Recent queries</div>
            <div class="card-body">
                <table class="table table-responsive-sm table-hover table-outline mb-0">
                    <thead class="thead-light">
                        <tr>
                            <th>Resource</th>
                            <th>Type</th>
                            <th>Query</th>
                            <th>Duration</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for query in recentqueries %}
                            <tr>
                                <td>{{ query.resourcename }}</td>
                                <td>{{ query.commandtype }}</td>
                                <td><code>{{ query.command }}</code></td>
                                <td>{{ query.duration }}</td>
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
